{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: ["config:recommended"],
  enabledManagers: ["custom.regex"],
  customManagers: [
    {
      customType: "regex",
      managerFilePatterns: ["Sonarr/VERSION"],
      matchStrings: ["^(?<currentValue>.*)$"],
      versioningTemplate: "loose",
      datasourceTemplate: "github-tags",
      depNameTemplate: "Sonarr/Sonarr",
    },
    {
      customType: "regex",
      managerFilePatterns: ["Radarr/VERSION"],
      matchStrings: ["^(?<currentValue>.*)$"],
      versioningTemplate: "loose",
      datasourceTemplate: "github-tags",
      depNameTemplate: "Radarr/Radarr",
    },
    {
      customType: "regex",
      managerFilePatterns: ["Prowlarr/VERSION"],
      matchStrings: ["^(?<currentValue>.*)$"],
      versioningTemplate: "loose",
      datasourceTemplate: "github-tags",
      depNameTemplate: "Prowlarr/Prowlarr",
    },
  ],
}
